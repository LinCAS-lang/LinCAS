/**
* Integer test:
* testing build in methods 
*/
printl "Testing Integers"; printl
printl 123 + 123
printl 123 - 123
printl 10 * 2
printl 5 \ 2
printl 5 / 2
printl 5 ^ 2
printl 5.to_s() + " (String)"
printl 5.to_f()
printl 

/**
* Float test
* testing built in methods
*/
printl "Testing Floats"; printl 
printl 0.25 + 1.0
printl 3.14 - 0.12
printl 0.2  * 0.2
printl 4.8  \ 2.0
printl 4.8  / 2.0
printl 4.8  ^ 2.0
printl 4.8.to_s() + " (String)"
printl 12.56.to_i()
printl

/**
* Mixing methods
*/
printl "Mixing methods"; printl
printl 12 + 0.25
printl 0.25 + 12
printl 12.4 - 3
printl 3 - 12.4
printl 12.0 \ 3
printl 4 \ 2.0
printl 12.0 / 3
printl 4 / 2.0

/**
* This line should raise a TypeError
*/
printl 3.0 + "string"
